{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "strict": true
    , "sourceMap": true,
  },
  "include": ["index.ts", "*.ts", "utils", "embeddings"],
  "exclude": ["node_modules"],
  "ts-node": {
    "esm": true
  }
}
